<Window x:Class="Address_Manager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Address_Manager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
 
        <StackPanel VerticalAlignment="Top" DockPanel.Dock="Top"  Orientation="Horizontal" Height="30" >
            <Label Content="Import CSV" HorizontalAlignment="Center"></Label>
            <TextBox IsEnabled="False" Width="250" Height="20" Margin="0 0 20 0"></TextBox>
            <Button Content="Browse" Width="100" Height="20" Margin="0 0 20 0"></Button>
            <Button Content="Change Mapping" Width="100" Height="20"></Button>

        </StackPanel>
        <DataGrid DockPanel.Dock="Left"></DataGrid>
      
    </DockPanel>
</Window>
